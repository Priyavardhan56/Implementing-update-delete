
@{
    ViewBag.Title = "AddDoctors";
    Layout = "~/Views/Shared/_Layout1.cshtml";
}

    <div class="container-fluid">
        <div class="container">
            <h3>Add Doctor</h3>

            <div class="row">
                <div class="col-sm-3">
                    <form>
                        <div class="form-input">
                            <label>Doctor Name</label>
                            <div class="form-control">
                            @{
                                if (Session["Email"] != null)
                                {
                                    @Session["Name"].ToString()
                                }
               
                            }
                                </div>
                        </div>
                </div>

                <div class="col-sm-1"></div>
                <div class="col-sm-3">
                    <div class="form-input">
                        <label>Doctor Designation</label>
                        <input class="form-control" id="employeeCode" type="number">
                    </div>
                </div>
                <div class="col-sm-1"></div>
                <div class="col-sm-3">
                    <div class="form-input">
                        <label>Entry By</label>
                        <input class="form-control" id="hospitalId" type="number">
                    </div>
                </div>
            </div>


            <div class="row mt-3">
                <div class="col-sm-3">
                    <form>
                        <div class="form-group ">

                            <label>Doctor Specialization</label>
                            <input type="text" class="form-control" id="tokenfield">
                        </div>
                </div>

                <div class="col-sm-1"></div>
                <div class="col-sm-3">
                    <div class="form-input">
                        <label>Doctor Qualification</label>
                        <input class="form-control" id="DoctorQualification" type="text">
                    </div>
                </div>
                <div class="col-sm-1"></div>
                <div class="col-sm-3">
                    <div class="form-input">
                        <label>Doctor Phone</label>
                        <input class="form-control" id="DoctorPhone" type="tel">
                    </div>
                </div>
            </div>

            <div class="row mt-3">
                <div class="col-sm-3">
                    <form>
                        <!-- <div class="form-input">
                    <label>Role Id</label>
                    <input class="form-control" id="roleId" type="number" >
                    </div> -->
                        @*<div class="form-input">
                            <label>Entry Date&Time</label>
                            <input class="form-control" id="entryDate&time" type="datetime-local">
                        </div>*@
                </div>

                <div class="col-sm-1"></div>
                <div class="col-sm-3">
                    <div class="form-input">
                        <label>Doctor Address</label>
                        <input class="form-control" id="DoctorAddress" type="text">
                    </div>
                </div>
                <div class="col-sm-1"></div>
                <div class="col-sm-3">
                    <div class="form-input">
                        <label>Doctor E-mail</label>
                        <input class="form-control" id="DoctorE-mail" type="text">
                    </div>
                </div>
            </div>


            <div class="row mt-3">
                <div class="col-sm-3">
                    <form>
                        <div class="form-input">
                            <label>City</label>
                            <input class="form-control" id="city" type="text">
                        </div>
                </div>

                <div class="col-sm-1"></div>
                <div class="col-sm-3">
                    <div class="form-input">
                        <label>State</label>
                        <input class="form-control" id="state" type="text">
                    </div>
                </div>
                <div class="col-sm-1"></div>
                <div class="col-sm-3">
                    <div class="form-input">
                        <label>Country</label>
                        <input class="form-control" id="country" type="text">
                    </div>
                </div>
            </div>

            <div class="row mt-3">
                <div class="col-sm-3">
                    <form>

                        <label class="display-block">Employee Status</label><br>
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="radio" name="status" id="product_active" value="option1" checked>
                            <label class="form-check-label" for="product_active">
                                Active
                            </label>
                        </div>
                        <div class="form-check form-check-inline">
                            <input class="form-check-input" type="radio" name="status" id="product_inactive" value="option2">
                            <label class="form-check-label" for="product_inactive">
                                Inactive
                            </label>
                        </div>
                        <!-- <div class="form-input">
                    <label>Designation</label>
                    <input class="form-control" id="designation" type="text" >
                    </div> -->

                </div>

                <div class="col-sm-1"></div>
                <div class="col-sm-3">
                    <!-- <div class="form-input">
                    <label>Entry By</label>
                    <input class="form-control" id="entryBy" type="text" >
                </div>    -->
                </div>
                <div class="col-sm-1"></div>
                <div class="col-sm-3">

                </div>
            </div>

            <br>


        </div>
        <div class="form-group mt-2" style="margin-left: 40%;">
            <button type="submit" class="btn btn-primary">
                Add
            </button>
            <button type="reset" class="btn btn-danger ">
                Cancel
            </button>
        </div>

        <br>



