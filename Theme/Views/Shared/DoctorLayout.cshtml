    <script type="text/javascript" src="~/Content/Assests/hia/docs-assets/js/typeahead.bundle.js" charset="UTF-8"></script>
    <script type="text/javascript" src="~/Content/Assests/hia/docs-assets/js/docs.min.js" charset="UTF-8"></script>
    <script>
        $(document).ready(function () {
            $('#tokenfield').tokenfield(
                {
                    autocomplete: {
                        source: ['fever', 'cold', 'cough', 'allergy'],
                        delay: 100
                    },
                    showAutocompleteOnFocus: true
                }
            );

            $('#tokenfield2').tokenfield({
                autocomplete: {
                    source: ['fever', 'cold', 'cough', 'allergy'],
                    delay: 100
                },
                showAutocompleteOnFocus: true
            }
            );


            $("form").submit(function (e) {
                e.preventDefault();
                $('.form-data').text($('#tokenfield').val());
                $('.form-data').append(', ' + $('#tokenfield2').val());
            });
        });
    </script>

