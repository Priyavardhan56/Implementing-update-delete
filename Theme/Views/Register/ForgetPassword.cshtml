

@{
    ViewBag.Title = "ForgetPassword";
   Layout = "~/Views/Shared/index.cshtml";
}

<head>
    <link href="@Url.Content("~/Content/login/")css/style.css" rel="stylesheet" type="text/css" />
    <!-- /custom style sheet -->
    <!-- fontawesome css -->
    <link href="@Url.Content("~/Content/login/")css/fontawesome-all.css" rel="stylesheet" />
    <!-- /fontawesome css -->
    <!-- google fonts-->
    <link href="//fonts.googleapis.com/css?family=Raleway:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i"
          rel="stylesheet">
</head>

<div class=" w3l-login-form"style="margin-top:4%">
    <form action="#" method="POST">
        <div class=" w3l-form-group">
            <label>Email:</label>
            <div class="group">
                <i class="fas fa-user"></i>
                <input type="text" id="user" class="form-control" placeholder="Email ID" required="required" />
            </div>
        </div>
        <button type="submit">Submit</button>
    </form>
    <p class=" w3l-register-p">@Html.ActionLink("Login", "Login")</p>
</div>

